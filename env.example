# WebDiário Student Portal - Configurações de Ambiente

# Configurações de autenticação Keycloak/OIDC
REACT_APP_KEYCLOAK_URL=http://localhost:8180
REACT_APP_KEYCLOAK_REALM=webdiario
REACT_APP_KEYCLOAK_CLIENT_ID=app-student-portal-client
REACT_APP_KEYCLOAK_CLIENT_SECRET=your-client-secret-here
REACT_APP_KEYCLOAK_REDIRECT_URI=http://localhost:3004

# Configurações da API de Segurança
REACT_APP_API_SECURITY_URL=http://localhost:8081

# URLs das aplicações
REACT_APP_WEBDIARIO_URL=http://localhost:3000
REACT_APP_STUDENT_PORTAL_URL=http://localhost:3004
REACT_APP_SECURITY_URL=http://localhost:3003

# Configurações de logging
REACT_APP_LOG_LEVEL=debug
REACT_APP_ENABLE_CONSOLE_LOGS=true
